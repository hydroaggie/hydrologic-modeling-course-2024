REGION brine_zone
  COORDINATES
    0.d0    0.d0    0.d0
    2640.d0 2640.d0 57.d0
  /
  FACE BOTTOM
END

REGION fresh_water
  COORDINATES
    0.d0    0.d0    70.d0
    2640.d0 2640.d0 57.d0
  /
  FACE BOTTOM
END

REGION west
  COORDINATES
    0.d0 0.d0    0.d0
    0.d0 2640.d0 70.d0
  /
  FACE WEST
END

REGION east
  COORDINATES
    2640.d0 0.d0    0.d0
    2640.d0 2640.d0 70.d0
  /
  FACE EAST
END

REGION top
  COORDINATES
    0.d0    0.d0    70.d0
    2640.d0 2640.d0 70.d0
  /
  FACE TOP
END

REGION well_u21
  COORDINATES 
    528.d0 792.d0 57.d0
    528.d0 792.d0 62.d0
  /
END

REGION well_u14
  COORDINATES 
    2112.d0 1848.d0 13.d0
    2112.d0 1848.d0 62.d0
  /
END

REGION well_u13
  COORDINATES 
    2112.d0 792.d0 21.d0
    2112.d0 792.d0 62.d0
  /
END

REGION Obs_pt_center
  COORDINATE 1320.d0 1320.d0 40.d0
END

REGION Obs_pt_center_lower
  COORDINATE 1320.d0 1320.d0 30.d0
END

REGION Obs_pt_west
  COORDINATE 100.d0 1320.d0 30.d0
END

REGION Obs_pt_east
  COORDINATE 1650.d0 1320.d0 30.d0
END

REGION Obs_pt_north
  COORDINATE 1320.d0 1320.d0 60.d0
END

REGION Obs_pt_south
  COORDINATE 1320.d0 330.d0 60.d0
END

#=========================== observation points ===============================
OBSERVATION
  REGION Obs_pt_center
  AT_CELL_CENTER 
  VELOCITY 
END
 
OBSERVATION
  REGION Obs_pt_center_lower
  AT_CELL_CENTER
  VELOCITY
END
 
OBSERVATION
  REGION Obs_pt_west
  AT_CELL_CENTER
  VELOCITY
END
 
OBSERVATION
  REGION Obs_pt_east
  AT_CELL_CENTER
  VELOCITY
END
 
OBSERVATION
  REGION Obs_pt_south
  AT_CELL_CENTER
  VELOCITY
END
 
OBSERVATION
  REGION Obs_pt_north
  AT_CELL_CENTER
  VELOCITY
END
 
#=========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
  /
  INTERPOLATION LINEAR
  DATUM 0.d0 0.d0 62.d0
  GRADIENT
    LIQUID_PRESSURE -0.0019 0. 0. #linear water table with a slope of -0.0038 -- a drop of 5 m across a distance of 2640 m
  /
  LIQUID_PRESSURE 101325.d0
END

FLOW_CONDITION colorado_river
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
   / 
   DATUM 0.d0 0.d0 60.d0
   LIQUID_PRESSURE 101325.d0
END

FLOW_CONDITION mill_creek
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
  /
  DATUM 0.d0 0.d0 67.d0
  LIQUID_PRESSURE 101325.0
END

FLOW_CONDITION east
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
  /
  DATUM 2640.d0 0.d0 68.d0 
  LIQUID_PRESSURE 101325.d0
END

FLOW_CONDITION no_flow
  TYPE
    LIQUID_FLUX NEUMANN
  /
  LIQUID_FLUX 0.0
END

FLOW_CONDITION evapotranspiration
  TYPE
    LIQUID_FLUX NEUMANN
  /
  LIQUID_FLUX -7.d-9
END

FLOW_CONDITION injection
  TYPE
    RATE SCALED_VOLUMETRIC_RATE NEIGHBOR_PERM #volumetric rate scaled by permeability
  /
  RATE 1.d5 m^3/day # positive-->injection
END

FLOW_CONDITION extraction
  TYPE
    RATE SCALED_VOLUMETRIC_RATE NEIGHBOR_PERM
  /
  RATE -100000.d0 m^3/day #negative-->extraction
END


#=========================== transport conditions =============================
TRANSPORT_CONDITION initial
  TYPE DIRICHLET_ZERO_GRADIENT
  CONSTRAINT initial
    CONCENTRATIONS
      Na+      1.d-10   T
      Cl-      1.d-10  T
      Tracer1  1.d-10     T
    /
  /
END

TRANSPORT_CONDITION west
  TYPE DIRICHLET_ZERO_GRADIENT
  CONSTRAINT west
    CONCENTRATIONS
      Na+      1.52d-2   T
      Cl-      1.52d-2   T
      Tracer1  1.d-10     T
    /
  /
END

TRANSPORT_CONDITION brine_initial
  TYPE DIRICHLET_ZERO_GRADIENT
  CONSTRAINT brine
    CONCENTRATIONS
     Na+      1.52d-2   T
     Cl-      1.52d-2   T
     Tracer1  1.d-10     T
     /
   /
END  

TRANSPORT_CONDITION injection
  TYPE DIRICHLET_ZERO_GRADIENT
  CONSTRAINT injection
    CONCENTRATIONS
      Na+      0.d0   T
      Cl-      0.d0   T
      Tracer1  1.d0   T
    /
  /
END

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION fresh_water
END

INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION brine_initial
  REGION brine_zone
END

# west boundary condition
BOUNDARY_CONDITION west
  FLOW_CONDITION colorado_river
  TRANSPORT_CONDITION west
  REGION west
END

# east boundary condition
BOUNDARY_CONDITION east
  FLOW_CONDITION east
  TRANSPORT_CONDITION initial
  REGION east
END

# top boundary condition
BOUNDARY_CONDITION top
  FLOW_CONDITION evapotranspiration
  TRANSPORT_CONDITION initial
  REGION top
END



 #source 
SOURCE_SINK injection_well
  FLOW_CONDITION injection
  TRANSPORT_CONDITION injection
  REGION well_u13
END

 #source 
SOURCE_SINK injection_well
  FLOW_CONDITION injection
  TRANSPORT_CONDITION injection
  REGION well_u13
END

SOURCE_SINK well_u21
  FLOW_CONDITION extraction
  TRANSPORT_CONDITION initial
  REGION well_u21
END

SOURCE_SINK well_u14
  FLOW_CONDITION extraction
  TRANSPORT_CONDITION initial
  REGION well_u14
END

#=========================== stratigraphy couplers ============================
STRATA
  REGION all
  MATERIAL soil1
END

STRATA
  REGION brine_zone
  MATERIAL brine_layer
END


