#Description: 3D groundwater flow and solute transport

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE RICHARDS
    /
    SUBSURFACE_TRANSPORT transport
      MODE GIRT
    /
  /
  CHECKPOINT
    PERIODIC TIMESTEP 50
    PERIODIC TIME .10 yr
  /
  #RESTART regional_doublet-restart.chk RESET_TO_TIME_ZERO
END

SUBSURFACE

#=========================== numerical methods ================================
NUMERICAL_METHODS FLOW

  NEWTON_SOLVER
    ITOL_UPDATE 1.d0     ! Convergences with max change in pressure is 1 Pa.
  /

END

#=========================== chemistry ========================================
CHEMISTRY
  PRIMARY_SPECIES
    Tracer1
    Na+
    Cl-
  /
  OUTPUT
    TOTAL
    ALL
  /
END


#=========================== discretization ===================================
GRID
  TYPE STRUCTURED
  NXYZ 50 50 20 
  BOUNDS
    0.d0 0.d0 0.d0
    2640.d0 2640.d0 70.d0 
  /
END

#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
  
END


#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.35d0
  TORTUOSITY 0.7d0
  CHARACTERISTIC_CURVES cc1
  PERMEABILITY
    PERM_X 3.1d-10
    PERM_Y 3.1d-10
    PERM_Z 3.1d-10
  /
END

MATERIAL_PROPERTY brine_layer
  ID 2
  POROSITY 0.35d0
  TORTUOSITY 0.7d0
  CHARACTERISTIC_CURVES cc1
  PERMEABILITY
    PERM_X 3.1d-10
    PERM_Y 3.1d-10
    PERM_Z 3.1d-10
  /
END

